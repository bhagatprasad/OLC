    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/font-awesome/css/all.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/owl.carousel/assets/owl.carousel.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/stylesheet.css")" />
