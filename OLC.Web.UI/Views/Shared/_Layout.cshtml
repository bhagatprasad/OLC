<body>

    <!-- Loading Spinner -->
    <div class="se-pre-con" style="display: none;">
        <div class="spinner-border text-primary" role="status">
            <span class="visually-hidden">Loading...</span>
        </div>
    </div>
    <!-- Preloader End -->
    <!-- Document Wrapper
    ============================================= -->
    <div id="main-wrapper">

        <!-- Header
        ============================================= -->
        <header id="header">
            <div class="container-fluid">
                <div class="header-row">
                    <div class="header-column justify-content-start">
                        <!-- Logo
                        ============================= -->
                        <div class="logo"> <a class="d-flex" href="@Url.Action("Index","Home",null)" title="Betalen"><img src="~/images/logo.png" alt="Betalen" /></a> </div>
                        <!-- Logo end -->
                        <!-- Collapse Button
                        ============================== -->
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#header-nav"> <span></span> <span></span> <span></span> </button><!-- Collapse Button end -->
                        <!-- Primary Navigation
                        ============================== -->
                        <partial name="~/Views/Shared/_MenuPartial.cshtml" />
                        <!-- Primary Navigation end -->
                    </div>

                    <div class="header-column justify-content-end">
                        <!-- Login & Signup Link
                        ============================== -->
                        <nav class="login-signup navbar navbar-expand">
                            <ul class="navbar-nav">
                                <li class="d-none"><a href="@Url.Action("ExternalLogin","Account",null)">Log In Google</a> </li>
                                <li><a  href="@Url.Action("Login","Account",null)">Login</a> </li>
                                <li class="align-items-center h-auto ml-sm-3"><a class="btn btn-primary d-none d-sm-block" href="@Url.Action("SignUp","Account",null)">Sign Up</a></li>
                            </ul>
                        </nav>
                        <!-- Login & Signup Link end -->
                    </div>
                </div>
            </div>
        </header>
        <!-- Header End -->
        <!-- Content
        ============================================= -->
        <div id="content">
            @RenderBody()
        </div>
        <!-- Content end -->
        <!-- Footer
        ============================================= -->
        <partial name="~/Views/Shared/_FooterPartial.cshtml" />
        <!-- Footer end -->

    </div>
    <!-- Document Wrapper end -->
    <!-- Back to Top
    ============================================= -->
    <a id="back-to-top" data-toggle="tooltip" title="Back to Top" href="javascript:void(0)"><i class="fa fa-chevron-up"></i></a>

    <!-- Video Modal
    ============================================= -->
    <div class="modal fade" id="videoModal" tabindex="-1" role="dialog">
        <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
            <div class="modal-content bg-transparent border-0">
                <button type="button" class="close text-white opacity-10 ml-auto mr-n3 font-weight-400" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <div class="modal-body p-0">
                    <div class="embed-responsive embed-responsive-16by9">
                        <iframe class="embed-responsive-item" id="video" allow="autoplay"></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Video Modal end -->
    <!-- Script -->
    <script src="@Url.Content("~/js/jquery/jquery.min.js")" asp-append-version="true" type="text/javascript"></script>
    <script src="@Url.Content("~/js/bootstrap/js/bootstrap.bundle.min.js")" asp-append-version="true" type="text/javascript"></script>
    @*<script src="~/vendor/bootstrap/js/bootstrap.js"></script>*@
    <script src="@Url.Content("~/js/owl.carousel/owl.carousel.min.js")" asp-append-version="true" type="text/javascript"></script>
    <script src="@Url.Content("~/js/theme.js")" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/Application/CommonController.js"></script>
    <script src="~/js/Application/StorageService.js"></script>
    @*<script src="~/js/theme.js"></script>*@
    <script src="https://kit.fontawesome.com/07c0b40a92.js" crossorigin="anonymous"></script>
    @await Component.InvokeAsync("Notyf")
    <script>
        $(window).on('load', function () {
            $(".se-pre-con").hide();
        });

        $(document).ajaxStart(function () {
            $(".se-pre-con").show();
        });

        $(document).ajaxComplete(function () {
            $(".se-pre-con").hide();
        });
    </script>
</body>

</html>
